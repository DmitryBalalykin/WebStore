@model System.Collections.Generic.List<WebStore.DomainNew.ViewModel.UserOrderViewModel>

@{
    ViewBag.Title = "Мои заказы";
    Layout = "Partial/_ProfileLayuot";
}

<h2>@ViewBag.Title</h2>

<table class="table table-bordered">
    <tr>
        <td>Номер</td>
        <td>Имя</td>
        <td>Адрес</td>
        <td>Телефон</td>
        <td>Сумма заказа</td>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
            <td>@item.Address</td>
            <td>@item.Phone</td>
            @*<td>@item.TotalSum.ToString("C")</td>*@
        </tr>
    }

</table>
